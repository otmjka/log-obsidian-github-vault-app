{
  "containerDefinitions": [
    {
      "name": "log-blog",
      "image": "akjmto/log-blog:latest",
      "portMappings": [
        {
          "name": "log-blog-3000",
          "containerPort": 3000,
          "hostPort": 3000,
          "protocol": "tcp",
          "appProtocol": "http"
        }
      ],
      "essential": true
    }
  ],
  "family": "log-blog-task-defenition",
  "executionRoleArn": "arn:aws:iam::762233768038:role/ecsTaskExecutionRole",
  "networkMode": "awsvpc",
  "volumes": [],

  "requiresCompatibilities": ["FARGATE"],
  "cpu": "1024",
  "memory": "3072"
}
